{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "leet-checkout",
    "description": "checkout leetcode problems automatically in github",
    "action": {
        "default_popup": "index.html",
        "default_title": "React Chrome Extension"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "notifications"
    ]    
}
